@import ../init as _

pingdom::check check-example
    name: "example-check"
    resolution: 5
    hostname: "s7.mydomain.com"
    send-notification-when-down: 7
    notify-again-every: 4
    notify-when-back-up: true
    tags: ["abc", "xyz"]

    http
        url: "/"
        port: 443
        encryption: true
        should-contain: "hello"
    end
end
